    "babel-plugin-transform-regenerator": "6.6.5",
    "babel-plugin-transform-runtime": "6.6.0",
    "babel-preset-es2015": "6.6.0",
    "chai": "3.5.0",
    "chai-subset": "1.2.2",
    "coveralls": "2.11.9",
    "eslint": "2.7.0",
    "eslint-plugin-babel": "3.2.0",
    "flow-bin": "0.22.1",
    "isparta": "4.0.0",
    "mocha": "2.4.5",
    "sane": "1.3.4"
